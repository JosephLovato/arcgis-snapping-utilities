{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "semi": "error",
        "object-curly-spacing": "off",
        "indent": [
            "error",
            4
        ]
    }
}